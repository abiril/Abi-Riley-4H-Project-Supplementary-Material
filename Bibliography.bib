doi = {10.1371/journal.pone.0114010},
editor = {Moormann, Ann M.},
file = {:C\:/Users/abiri/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Phillips-Howard et al. - 2014 - Deaths Ascribed to Non-Communicable Diseases among Rural Kenyan Adults Are Proportionately Increasing Ev.pdf:pdf},
