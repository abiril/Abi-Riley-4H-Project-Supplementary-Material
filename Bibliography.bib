@article{Clarke2019,
abstract = {Health is more than the absence of disease. It is also more than a biological phenomenon. It is inherently social, psychological, cultural and historical. While this has been recognised by major health actors for decades, open questions remain as to how to build systems that reflect the complexity of health, disease and sickness, and in a context that is increasingly technologised. We argue that an urgent change of approach is necessary. Methods and concepts from the humanities and social science must be embedded in the concepts and methods of the health sciences if we are to promote sustainable interventions capable of engaging with the recognised complexity of health, disease and sickness. Our vision is one of radical interdisciplinarity, integrating aspects of biological, psychological, social and humanities approaches across areas of urgent health need. Radical interdisciplinarity, we argue, entails the practical, methodological and conceptual integration of these approaches to health.},
author = {Clarke, Brendan and Ghiara, Virginia and Russo, Federica},
doi = {10.1136/bmjopen-2019-030286},
