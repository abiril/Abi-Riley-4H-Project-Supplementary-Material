pages = {181--199},
publisher = {International Statistical Institute},
title = {{Hierarchical Models in Environmental Science}},
url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1751-5823.2003.tb00192.x},
volume = {71},
year = {2003}
}
