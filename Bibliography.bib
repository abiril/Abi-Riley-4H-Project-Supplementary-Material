pmid = {30212102},
publisher = {The World Bank},
title = {{Major Infectious Diseases: Key Messages from Disease Control Priorities, Third Edition}},
url = {https://www.ncbi.nlm.nih.gov/books/NBK525197/},
year = {2017}
}
